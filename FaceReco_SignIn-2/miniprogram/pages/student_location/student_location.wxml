<!--pages/student_location/student_location.wxml-->
<view>
  <map id="map" longitude="{{longitude}}" latitude=" {{latitude}}" markers="{{markers}}" scale="13" controls="{{controls}}" bindcontroltap="controltap" bindmarkertap="markertap" polyline="{{polyline}}" circles="{{circles}}" bindregionchange="regionchange"  show-location style="width: 100%; height: 65vh;"></map>
  <view class="location_txt">Location</view>
  <view>\n</view>
  <view>
    <button type="default" style="margin-top:10px" bindtap="jump2FaceReco">确定</button>
  </view>
</view>